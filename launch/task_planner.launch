<?xml version="1.0"?>
<launch>
  <node pkg="push_vs_grasp" name="task_planner" type="task_planner" output="screen">
    <param name="obj_x_min" value="-0.55"/>
    <param name="obj_x_max" value="-0.2"/>
    <param name="obj_y_min" value="-0.3"/>
    <param name="obj_y_max" value="0.3"/>

    <param name="goal_x_min" value="-0.2"/>
    <param name="goal_x_max" value="0.2"/>
    <param name="goal_y_min" value="-0.3"/>
    <param name="goal_y_max" value="-0.55"/>

    <param name="red_radius" value="0.1"/>
    <param name="blue_radius" value="0.1"/>
    <param name="object_radius" value="0.03"/>
        
    <param name="num_obj_min" value="5"/>
    <param name="num_obj_max" value="5"/>
    <param name="runs_per_obj" value="10"/>

    <param name="timeout_secs" value="120"/>
    <param name="log_folder" value="$(find push_vs_grasp)/log/"/>
  </node>
</launch>
