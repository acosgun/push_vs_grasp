<?xml version="1.0"?>
<launch>

  <arg name="paused" default="true"/>
  <arg name="gui" default="true"/>

  <!-- Start Sim -->
  <include file="$(find push_vs_grasp)/launch/empty_world.launch">
    <arg name="world_name" value="$(find push_vs_grasp)/config/empty.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="gui" value="$(arg gui)"/>
  </include>
   
</launch>
